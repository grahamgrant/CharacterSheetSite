            elif skill == "survival":
                player.surskl=True
                print "You are profiecient in " + skill + "!"
            else:
                print "whoopsies"
                player.acrskl=False
                player.ahskl=False
                player.arcskl=False
                player.atskl=False
                player.decskl=False
                player.hiskl=False
                player.inskl=False
