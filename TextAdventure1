            print "Sorry, havent added that one yet. Try again."
            chclass()

    player.my_class=chclass()

    print "OK, so now you may choose which skills to be proficient with."

    def addskill():
        if player.my_class == "fighter":
            player.hp = 10 + mod(player.CON)
            skill=raw_input("Choose a skill from Acrobatics, Animal Handling, Athletics, History, Insight, Intimidation, Perception, and Survival").lower()
            if skill == "acrobatics":
                player.acrskl=True
                print "You are profiecient in " + skill + "!"
            elif skill == "animal handling":
                player.ahskl=True
                print "You are profiecient in " + skill + "!"
            elif skill == "athletics":
                player.atskl=True
