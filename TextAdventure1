print "Welcome to the 1028 text adventure!"
print "This is a text RPG using a combo of pathfiner and DnD 5e, mostly 5e. This game is Hardmode. You die, your character is deleted, no saves, nothing. I'll try to walk you through each stup but I assume you have acces to some knoledge of mechanics so if you get lost, check out 5esrd.com or d20pfsrd.com. Good luck!"

def genChar():
    print "Lets  begin by creating your character."

    import random
    bonus=""
    penalty=""
    my_race= ""
    def race():
        answer = raw_input("First, which race would you like to be?").lower()
        if answer == "human" or answer == "half-orc" or answer == "half-elf":
            return answer
            print "You are a " + answer + "!"
        elif answer == "elf" or answer == "dwarf":
            return answer
            print "You are a " + answer + "!"
        elif answer == "gnome" or answer == "halfling":
            return answer
            print "You are a " + answer + "!"
        else:
            print "Sorry, havent added that one yet. Try again."
            race()

    player.my_race = race()

    print "Ok, now lets get some stats. These will be genereated with the 4D6H3 method and are random so if they suck you could just restart but whats the fun in that, right?"

    def getStat():
        da = random.randrange(1,7)
        db = random.randrange(1,7)
        dc = random.randrange(1,7)
        dd = random.randrange(1,7)
        stat= da + db + dc + dd
        low= min(da , db ,dc ,dd)
        return stat - low

    Stat1 = getStat()
    Stat2 = getStat()
    Stat3 = getStat()
    Stat4 = getStat()
    Stat5 = getStat()
    Stat6 = getStat()

    if player.my_race=="elf":
        bonus = "DEX and INT"
        penalty="CON."
    elif player.my_race=="dwarf":
        bonus = "CON and WIS"
        penalty="CHA."
    elif player.my_race=="halfling":
        bonus="DEX and CHA"
        penalty = "STR."
    elif player.my_race=="gnome":
        bonus="CON and CHA"
        penalty = "STR."
    elif player.my_race=="human" or player.my_race=="half-elf" or player.my_race == "half-orc":
        bonus = "one of your choice"
        penalty ="nothing!"

    print str(Stat1) + " " + str(Stat2) + "  " + str(Stat3) + " " + str(Stat4) + " " +     str(Stat5) + " " + str(Stat6)
    print "These are your stats!"
    print "Because you are a "+str(player.my_race)+" you get a +2 to "+bonus+" and player.a -2 to "+penalty

    class gone(object):
        onegon = False
        twgon = False
        thrgon = False
        forgon = False
        fivgon = False
        sixgon = False
    use=gone()
    strdone=False
    dexdone==False
    condone=False
    intdone=False
    wisdone=False
    chadone=False
    def getSTATS(Stat1, Stat2, Stat3, Stat4, Stat5, Stat6):
        if not strdone:
            mstr = raw_input("Which value would you like to assign to your STR?")
            if mstr == str(Stat1) and use.onegon == False:
                player.STR=Stat1
                print("Your Strength is " +str(player.STR))
                use.onegon=True
                strdone=True
            elif mstr == str(Stat2) and use.twgon == False:
                player.STR = Stat2
                print("Your Strength is " +str(player.STR))
                use.twgon=True
                strdone=True
            elif mstr == str(Stat3) and use.thrgon == False:
                player.STR = Stat3
                print("Your Strength is " +str(player.STR))
                use.thrgon=True
                strdone=True
            elif mstr == str(Stat4) and use.forgon == False:
                player.STR = Stat4
                print("Your Strength is " +str(player.STR))
                use.forgon=True
                strdone=True
            elif mstr == str(Stat5) and use.fivgon == False:
                player.STR = Stat5
                print("Your Strength is " +str(player.STR))
                use.fivgon=True
                strdone=True
            elif mstr == str(Stat6) and use.sixgon == False:
                player.STR = Stat6
                print("Your Strength is " +str(player.STR))
                use.sixgon=True
                strdone=True
            else:
                print "sorry, use a stat you actually rolled"
                use.onegon = False
                use.twgon = False
                use.thrgon = False
                use.forgon = False
                use.fivgon = False
                use.sixgon = False
                getSTATS(Stat1, Stat2, Stat3, Stat4, Stat5, Stat6)
        if not dexdone:
            mstr = raw_input("Which value would you like to assign to your DEX?")
            if mstr == str(Stat1) and use.onegon == False:
