    def addHUM():
        if player.my_race=="human" or player.my_race=="half-elf" or player.my_race == "half-orc":
            print "Because you are a " + player.my_race+", you may add +2 to any stat"
            madd = raw_input("Which Stat would you like to add to?").upper()
            if madd == "STR":
                player.STR=player.STR+2
                print "Your Strength is now " + str(player.STR)+ "!"
            elif madd == "DEX":
                player.DEX=player.DEX+2
                print "Your Dexterity is now " + str(player.DEX)+ "!"
            elif madd == "CON":
                player.CON=player.CON+2
                print "Your Constitution is now " + str(player.CON)+ "!"
            elif madd == "INT":
                player.INT=player.INT+2
                print "Your Intelligence is now " + str(player.INT)+ "!"
            elif madd == "WIS":
                player.WIS=player.WIS+2
                print "Your Wisdom is now " + str(player.WIS)+ "!"
            elif madd == "CHA":
                player.CHA=player.CHA+2
                print "Your Charisma is now " + str(player.CHA)+ "!"
            else:
                print "that's not a stat bro"
                addHUM()
    addHUM()

    if player.my_race=="gnome":
        player.STR=player.STR-2
        player.CHA=player.CHA+2
        player.CON=player.CON+2
    elif player.my_race=="halfling":
        player.STR=player.STR-2
        player.CHA=player.CHA+2
        player.DEX=player.DEX+2
    elif player.my_race=="elf":
        player.DEX=player.DEX+2
        player.INT=player.INT+2
        player.CON=player.CON-2
    elif player.my_race=="dwarf":
        player.CON=player.CON+2
        player.WIS=player.WIS+2
        player.CHA=player.CHA-2

    print"Cool, so now your stats are STR:"+str(player.STR)+" DEX:"+str(player.DEX)+" CON:"+str(player.CON)+" INT:"+str(player.INT)+" WIS:"+str(player.WIS)+" CHA:"+str(player.CHA)
    print "It's time to pick a class!"
    def chclass():
        answer = raw_input("Which class would you like to be? Only core classes.").lower()
        if answer == "fighter" :
            return answer
            print "You are a " + answer + "!"
        elif answer == "paladin":
            return answer
            print "You are a " + answer + "!"
        elif answer == "cleric":
            return answer
            print "You are a " + answer + "!"
        elif answer == "druid":
            return answer
            print "You are a " + answer + "!"
        elif answer == "barbarian":
            return answer
            print "You are a " + answer + "!"
