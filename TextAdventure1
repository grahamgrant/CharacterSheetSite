def mod(skill):
    skill=skill-10
    abtg=skill%2
    skill=skill-abtg
    skill=skill/2
    return skill
def d20():
    return random.randrange(1,21)
def d12():
    return random.randrange(1,13)
def d10():
    return random.randrange(1,11)
def d8():
    return random.randrange(1,9)
def d6():
    return random.randrange(1,7)
def d4():
    return random.randrange(1,5)
class char:
    name=""
    STR=0
    DEX=0
    CON=0
    INT=0
    WIS=0
    CHA=0
    my_race=""
    my_class=""
    hp=0
    ac=0
    level=1
    hd=0
    saves=[0,0,0,0,0,0]
    role=""
    def getprof(self):
        return self.level/5+2
    jack=False
    acrskl=False
    ahskl=False
    arcskl=False
    atskl=False
    decskl=False
    hiskl=False
    inskl=False
    intskl=False
    invskl=False
    medskl=False
    natskl=False
    perskl=False
    perfskl=False
    persskl=False
    relskl=False
    sohskl=False
    sthskl=False
    surskl=False
    acrsklex=False
    ahsklex=False
    arcsklex=False
    atsklex=False
    decsklex=False
    hisklex=False
    insklex=False
    intsklex=False
    invsklex=False
    medsklex=False
    natsklex=False
    persklex=False
    perfsklex=False
    perssklex=False
    relsklex=False
    sohsklex=False
    sthsklex=False
    sursklex=False
    def getacr(self):
        if acrsklex==True:
            return self.DEX+getprof()*2
        elif acrskl==True:
            return self.DEX+getprof()
        elif jack==True:
            return self.DEX+getprof()/2
        else:
            return self.DEX
    def getah(self):
        if ahsklex==True:
            return self.CHA+getprof()*2
        elif ahskl==True:
            return self.CHA+getprof()
        elif jack==True:
            return self.CHA+getprof()/2
        else:
            return self.CHA
    def getarc(self):
        if arcsklex==True:
            return self.INT+getprof()*2
        elif arcskl==True:
            return self.INT+getprof()
        elif jack==True:
            return self.INT+getprof()/2
        else:
            return self.INT
    def getat(self):
        if atsklex==True:
            return self.STR+getprof()*2
        elif atskl==True:
            return self.STR+getprof()
        elif jack==True:
            return self.STR+getprof()/2
        else:
            return self.STR
    def getdec(self):
        if decsklex==True:
            return self.CHA+getprof()*2
        elif decskl==True:
            return self.CHA+getprof()
        elif jack==True:
            return self.CHA+getprof()/2
        else:
            return self.CHA
    def gethi(self):
        if hisklex==True:
            return self.INT+getprof()*2
        elif hiskl==True:
            return self.INT+getprof()
        elif jack==True:
            return self.INT+getprof()/2
        else:
            return self.INT
    def getin(self):
        if insklex==True:
            return self.WIS+getprof()*2
        elif inskl==True:
            return self.WIS+getprof()
        elif jack==True:
            return self.WIS+getprof()/2
        else:
            return self.WIS
    def getint(self):
        if intsklex==True:
            return self.CHA+getprof()*2
        elif intskl==True:
            return self.CHA+getprof()
        elif jack==True:
            return self.CHA+getprof()/2
        else:
            return self.CHA
    def getinv(self):
        if invsklex==True:
            return self.INT+getprof()*2
        elif invskl==True:
            return self.INT+getprof()
        elif jack==True:
            return self.INT+getprof()/2
        else:
            return self.INT
    def getmed(self):
        if medsklex==True:
            return self.WIS+getprof()*2
        elif medskl==True:
            return self.WIS+getprof()
        elif jack==True:
            return self.WIS+getprof()/2
        else:
            return self.WIS
    def getnat(self):
        if natsklex==True:
            return self.INT+getprof()*2
        elif natskl==True:
            return self.INT+getprof()
        elif jack==True:
            return self.INT+getprof()/2
        else:
            return self.INT
    def getpers(self):
        if perssklex==True:
            return self.CHA+getprof()*2
        elif persskl==True:
            return self.CHA+getprof()
        elif jack==True:
            return self.CHA+getprof()/2
        else:
            return self.CHA
    def getperf(self):
        if perfsklex==True:
            return self.CHA+getprof()*2
        elif perfskl==True:
            return self.CHA+getprof()
        elif jack==True:
            return self.CHA+getprof()/2
        else:
            return self.CHA
    def getrel(self):
        if relsklex==True:
            return self.INT+getprof()*2
        elif relskl==True:
            return self.INT+getprof()
        elif jack==True:
            return self.INT+getprof()/2
        else:
            return self.INT
    def getsoh(self):
        if sohsklex==True:
            return self.DEX+getprof()*2
        elif sohskl==True:
            return self.DEX+getprof()
        elif jack==True:
            return self.DEX+getprof()/2
        else:
            return self.DEX
    def getsth(self):
        if sthsklex==True:
            return self.DEX+getprof()*2
        elif sthskl==True:
            return self.DEX+getprof()
        elif jack==True:
            return self.DEX+getprof()/2
        else:
            return self.DEX
    def getsur(self):
        if sursklex==True:
            return self.WIS+getprof()*2
        elif surskl==True:
            return self.WIS+getprof()
        elif jack==True:
            return self.DEX+getprof()/2
        else:
            return self.DEX

weapons = {
    'martial': ("greataxe", "greatsword", "longsword"),
    'simple': ("dagger", "quarterstaff", "shortsword")
}
